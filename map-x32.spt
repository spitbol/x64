g.loop
	s break(' ') . name ' ' =	:f(g.loop.end)
	global_count = global_count + 1
	global_name_table[global_count] = name
	global_index_table[name] = global_count
	global_names = array(global_count)
					:(g.loop)
g.loop.end
	i = 0
gg.loop
	gt(i = I = 1, global_count)	:s(gg.loop.end)
	global_names[i] = name 
gg.loop.end

					:(return)
finis
	&dump = 0
