	&trim = 1
	&dump = 0
	isglobal = 0
	int0 = INTEGER(CHAR('0'))
	define('intof(off)val,i,str,c')
	define('global_init()')
	digits = table()
	digits['0'] = 0
	digits['1'] = 1
	digits['2'] = 2
	digits['3'] = 3
	digits['4'] = 4
	digits['5'] = 5
	digits['6'] = 6
	digits['7'] = 7
	digits['8'] = 8
	digits['9'] = 9
	digits['a'] = 10
	digits['b'] = 11
	digits['c'] = 12
	digits['d'] = 13
	digits['e'] = 14
	digits['f'] = 15

	global_init()

loop
	line = input			:f(finis)
	ne(size(line),16)		:s(loop)
	line LEN(8) . offset  ' '  ANY('dD') 
+		' ' rem . name	:f(loop) 
	IDENT(global_index_table[name])	:s(loop)
	ioffset = intof(offset)
	base = IDENT(name, 'C_AAA') ioffset
*	output  = IDENT(name, 'C_AAA') 'base ' ioffset
 	output = name ' ' (ioffset - base)
	last_name = name
	DIFFER(name,'W_YYY')		:s(loop)
*	output = 'last name ' last_name
loop.end
	:(finis)

intof
	intof = 0
	i = 0
	str = REVERSE(off)
	str = off
*	output = 'intof arg ' off
intof.1
	gt(i = i + 1, 8)		:s(return)
	c = SUBSTR(str,i,1)
	intof = intof * 16 + digits[c]  
					:(intof.1)
	intof = intof - base		:(return)
global_init
	global_index_table = TABLE(600)
	global_name_table = TABLE(600)
	s = 
